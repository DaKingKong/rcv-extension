{
    "manifest_version": 3,
    "name": "RCV Demo",
    "description": "",
    "version": "0.0.1",
    "icons": {
      "16": "/images/logo16.png",
      "32": "/images/logo32.png",
      "48": "/images/logo48.png",
      "128": "/images/logo128.png"
    },
    "permissions": [
        "scripting",
        "activeTab"
    ],
    "action": {
      "default_icon": {
        "16": "/images/logo16.png",
        "32": "/images/logo32.png",
        "48": "/images/logo48.png",
        "128": "/images/logo128.png"
      }
    },
    "host_permissions": [
        "https://www.figma.com/*"
    ],
    "background": {
        "service_worker": "sw.js"
    },
    "content_scripts": [
        {
            "js": [
                "content.js",
                "ringcentral.js"
            ],
            "matches": [
                "https://www.figma.com/*"
            ],
            "all_frames": false
        }
    ]
}